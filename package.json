    "@smui/textfield": "^8.0.0-beta.3",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/adapter-vercel": "^6.0.0",
    "@sveltejs/kit": "^2.48.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@vitest/coverage-v8": "^4.0.4",
